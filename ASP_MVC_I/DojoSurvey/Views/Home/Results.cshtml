@model DojoSurvey.Models.User

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Results</title>
        <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css"
        integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">
</head>
<body>
    <h1 style="margin-left: 30px">Submitted Information</h1>
    <form action="" method="">
        <table>
            <tr>
                <th></th>
                <th></th>
            </tr>

            <tr>
                <td><h3>Name: </h3></td>
                <td><h3>&nbsp;@ViewBag.name</h3></td>
            </tr>
            <tr>
                <td><h3>Dojo Location: </h3></td>
                <td><h3>&nbsp;@ViewBag.location</h3></td>
            </tr>
            <tr>
                <td><h2>Favorite Language: </h3></td>
                <td><h2>&nbsp;@ViewBag.language</h3></td>
            </tr>
            <tr>
                <td><h3>Comment (optional): </h3></td>
                <td><h3>&nbsp;@ViewBag.comment</h3></td>
            </tr>
        </table>
    </form>
</body>
</html>